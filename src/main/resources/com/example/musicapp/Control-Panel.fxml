<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.media.MediaView?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.musicapp.ControlPanelController"
            prefHeight="800.0" prefWidth="600.0">

    <ScrollPane style="-fx-background-color: #ffffff; -fx-padding: 10; -fx-border-color: #cccccc; -fx-border-width: 1px;">
        <content>
            <VBox fx:id="Main" spacing="10" alignment="CENTER">
                <TilePane fx:id="adsPane" alignment="CENTER" hgap="10" prefTileHeight="80" prefTileWidth="250" vgap="10"
                          style="-fx-background-color: #f0f0f0; -fx-padding: 20; -fx-border-color: #cccccc; -fx-border-width: 1px;">
                </TilePane>
                <Pane fx:id="adsBtns"></Pane>
                <TilePane fx:id="dirPane" alignment="CENTER" hgap="10" prefTileHeight="80" prefTileWidth="250" vgap="10"
                          style="-fx-background-color: #f0f0f0; -fx-padding: 20; -fx-border-color: #cccccc; -fx-border-width: 1px;">
                </TilePane>
                <VBox fx:id="usersBox">
                    <HBox >
                        <ScrollPane fitToWidth="true" fitToHeight="true" style="-fx-background-color: #f0f0f0; -fx-padding: 10; -fx-border-color: #cccccc; -fx-border-width: 1px;">
                            <content>
                                <TilePane fx:id="usersPaneScrolle" alignment="CENTER" hgap="10" vgap="10"
                                          style="-fx-background-color: #f0f0f0; -fx-padding: 20; -fx-border-color: #cccccc; -fx-border-width: 1px;">
                                </TilePane>
                            </content>
                        </ScrollPane>

                    </HBox>
                    <Pane fx:id="userBtns">

                    </Pane>
                </VBox>


            </VBox>
        </content>
    </ScrollPane>
</AnchorPane>

